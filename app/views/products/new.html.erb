<%= form_for @product do |form| %>


<%= form.label :ProductName, 'Product Name' %>
<%= form.text_field :ProductName %>
<br>


<%= form.label :quantity %>
<%= form.select :quantity, options_for_select(%w[80 160 240 320]) %>
<br>

<%= form.label :price %>
<%= form.text_field :price %>
<br>

<%= form.submit %>

<% end %>